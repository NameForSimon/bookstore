<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.bookstore.dao.OrderMapper">
	<insert id="pushOrder" parameterType="com.springboot.bookstore.bean.Order" >
	 insert into order(sumPrice,cid) values(#{sumPrice},#{cid})
	</insert>
	
	<select id="selectOrders" parameterType="java.lang.String" resultType="com.springboot.bookstore.bean.Manager">
	select * from manager where name=#{name}
	</select>
</mapper>